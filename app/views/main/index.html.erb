<div class="container">
  <div class="main-img-container">
    <h1 id="img-header"></h1>
    <h2>00:00:00</h2>
    <div class="img-wrapper">
      <div class="image-transition">
        <img></img>
        <img></img>
      </div>
      <button id="left-arrow"><%= fa_icon("chevron-left", type: :fas) %></button>
      <button id="right-arrow"><%= fa_icon("chevron-right", type: :fas) %></button>
    </div>
  </div>
  <div class="highscores">
    <h2>High Scores</h2>
    <div class="scores">
      <table class="best-list">
        <tr>
          <th>Name</th>
          <th>Score</th>
        </tr>
      </table>
    </div>
  </div>
</div>
<div class="login">
  <h2>Please enter your name</h2>
  <%= form_tag do %>
    <%= text_field_tag "username" %>
    <%= submit_tag "Confirm", :id => "submit" %>
  <% end %>
</div>
<div class="foundIt">
</div>
